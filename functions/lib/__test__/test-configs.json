{
  "defaults": {
    "destinations": [{"url": "http://google.com"}]
  },
  "missing-manditory": {},
  "single-transformation": {
    "destinations": [{"url": "http://google.com"}],
    "filters": [],
    "transformations": [
      "{ message: join(' ', ['you live in', locations[0].name]), city: 'asdf' }"
    ]
  },

  "multi-transformation": {
    "destinations": [{"url": "http://google.com"}],
    "filters": [],
    "transformations": [
      "{ message: join(' ', ['you live in', locations[0].name]) }",
      "{ locations: '' }"
    ]
  },

  "single-filter": {
    "destinations": [{"url": "http://google.com"}],
    "filters": [
      "location[0].name == `asdf`"
    ]
  },

  "multi-filter": {
    "destinations": [{"url": "http://google.com"}],
    "filters": [
      "locations[3]",
      "location[0].name == `asdf`"
    ]
  },

  "single-filter-pass": {
    "destinations": [{"url": "http://google.com"}],
    "filters": [
      "locations[3]"
    ]
  },

  "single-extraction": {
    "destinations": [{"url": "http://google.com"}],
    "filters": [],
    "extractions": [
      "{ message: join(' ', ['you live in', locations[0].name]), city: {} }",
      "{ city: locations[0].name }"
    ]
  },

  "multi-destinations": {
    "destinations": [
      {
        "url": "http://jsonplaceholder.typicode.com/posts/1",
        "contentType": "application/json",
        "filters": []
      },
      {
        "url": "http://jsonplaceholder.typicode.com/posts/1",
        "method": "POST",
        "contentType": "application/x-www-form-urlencoded",
        "filters": []
      }
    ],
    "filters": [],
    "extractions": [
      "{ message: join(' ', ['you live in', locations[0].name]) }",
      "{ city: locations[0].name }"
    ]
  },

  "everything": {
    "destinations": [
      {
        "url": "http://jsonplaceholder.typicode.com/posts/1",
        "filters": ["locations[0].name == 'Seattle'"]
      },
      {
        "url": "http://jsonplaceholder.typicode.com/posts/2",
        "method": "PUT",
        "contentType": "application/x-www-form-urlencoded",
        "filters": ["locations[0].name == 'Not Found'"]
      }
    ],
    "filters": ["locations[3]"],
    "transformations": [
      "{ someCities: locations[1:3], cityCount: length(locations) }",
      "{ keys: keys(@) }"
    ],
    "extractions": [
      "{ message: join(' ', ['you live in', locations[0].name]) }",
      "{ someStates: someCities[].state }",
      "{ k: keys }"
    ]
  },

  "extractions-transformations-on-destinations": {
    "destinations": [
      {
        "url": "http://jsonplaceholder.typicode.com/posts/1",
        "extractions": [],
        "transformations": []
      },
      {
        "url": "http://jsonplaceholder.typicode.com/posts/3"
      }
    ],
    "filters": [
      "branch == `master` || branch == `important-branch`",
      "status_message == `Fixed` || status_message == `Broken`"
    ],
    "extractions": [
      "{ message: join(' ', ['Tests are', status_message, 'on', branch, 'b/c:', message]) }"
    ]
  },

  "travis-ci": {
    "json-embeded-form-parameter": "payload",
    "destinations": [
      {
        "url": "http://jsonplaceholder.typicode.com/posts/1"
      }
    ],
    "filters": [
      "branch == `master` || branch == `important-branch`",
      "status_message == `Fixed` || status_message == `Broken`"
    ],
    "extractions": [
      "{ message: join(' ', ['Tests are', status_message, 'on', branch, 'b/c:', message]) }"
    ]
  }
}
