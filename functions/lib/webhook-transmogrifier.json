{
  "travis-ci": {
    "json-embeded-form-parameter": "payload",
    "destinations": [
      {
        "url": "https://hooks.slack.com/services/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"
      }
    ],
    "filters": [
      "branch == `master` || branch == `boston` || branch == `boston.upload-bugs`",
      "status_message == `Fixed` || status_message == `Broken` || status_message == `Still Failing`"
    ],
    "extractions": [
      "{ text: join(' ', ['Tests are *', status_message, '* on `', branch, '`  >>', message]) }"
    ]
  },
  "github-pr": {
    "destinations": [{ "url": "https://hooks.slack.com/services/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" }],
    "filters": [
      "action == `labeled`",
      "ends_with(label.name, `reviewable`)"
    ],
    "extractions": [
      "{ text: join('', ['<', pull_request.html_url, '|', pull_request.title '> by ', pull_request.user.login]) }"
    ]
  }
}
